    "def n_prime(n):\n",
    "    lst = []\n",
    "    num = 2\n",
    "\n",
    "    while len(lst) < n:\n",
    "        for i in range(2,int(num ** 0.5)+1):\n",
    "            if num % i == 0:\n",
    "                break\n",
    "        else:\n",
    "            lst.append(num)\n",
    "        num += 1\n",
    "    return lst\n",
    "       "
   ]
